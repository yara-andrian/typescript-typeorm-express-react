  "author": "AK",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.yaml dist/",
    "dev": "yarn clean && yarn copy-files && tsc-watch --onSuccess \"node ./dist/server.js\"",
    "test": "jest",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config"
  },
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.31",
    "@types/pino": "^6.3.6",
    "@types/swagger-ui-express": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.12.1",
    "helmet": "^4.4.1",
    "js-yaml": "^4.0.0",
    "pg": "^8.5.1",
    "pino": "^6.11.1",
    "pino-http": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^5.1.7",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/pino-http": "^5.4.0",
    "@types/supertest": "^2.0.10",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "tsc-watch": "^4.2.9"
  }
}
